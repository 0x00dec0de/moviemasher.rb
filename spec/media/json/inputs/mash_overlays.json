{
	"type": "mash",
	"source": {
		"quantize": 10,
		"label": "Overlays",
		"backcolor": "red",
		"media": [{
			"id": "drummers",
			"type": "video",
			"audio": 0,
			"duration": 10,
			"source": "video/drummers.mov"
		},{
			"label": "Overlay",
			"type": "transform",
			"id": "com.moviemasher.transform.overlay",
			"properties": { 
				"scale": { "type": "size1", "value": 0.50 },
				"left": { "type": "pixel", "value": 0 },
				"top": { "type": "pixel", "value": 0 }
			},
			"filters": [
				{
					"id": "scale",
					"parameters": [
						{
							"name": "width",
							"value":"mm_horz(scale)"
						},{
							"name": "height",
							"value":"mm_vert(scale)"
						}
					]
				},
				{
					"id": "setsar",
					"parameters": [
						{
							"name": "sar",
							"value":"1"
						},{
							"name": "max",
							"value":"1"
						}
					]
				},
				{
					"id": "overlay",
					"parameters": [
						{
							"name": "x",
							"value":"left*(mm_width-overlay_w)"
						},{
							"name": "y",
							"value":"top*(mm_height-overlay_h)"
						}
					]
				}
			]
		},
		{
			"label": "Color",
			"type": "theme",
			"id": "com.moviemasher.theme.color",
			"properties": { 
				"color": { "type": "rgb", "value": "red" }
			},
			"filters": [{
				"id": "color",
				"parameters":[{
					"name": "color",
					"value":"color"
				}]
			}]
		}
		],
		"tracks": {
			"video": [
				{
					"type": "video",
					"clips": [{
						"id": "com.moviemasher.theme.color",
						"effects": [],
						"frame": 0,
						"length": 50,
						"color": "blue"
					},{
						"id": "com.moviemasher.theme.color",
						"effects": [],
						"frame": 50,
						"length": 70,
						"color": "green"
					},{
						"id": "com.moviemasher.theme.color",
						"effects": [],
						"frame": 120,
						"length": 20,
						"color": "yellow"
					}]
				},{
					"type": "video",
					"clips": [{
						"id": "drummers",
						"effects": [],
						"frame": 20,
						"length": 70,
						"transform": {
							"id": "com.moviemasher.transform.overlay",
							"scale": 0.5,
							"left": 0.5,
							"top": 0.5
						}
					},
					{
						"id": "drummers",
						"effects": [],
						"trim": 30,
						"frame": 100,
						"length": 30,
						"transform": {
							"id": "com.moviemasher.transform.overlay",
							"scale": 0.5,
							"left": 1,
							"top": 0
						}
					}]
				}
			],
			"audio": [
				{
					"type": "audio",
					"clips": []
				}
			]
		}
	}
}